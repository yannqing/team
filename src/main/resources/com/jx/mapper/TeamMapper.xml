<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jx.mapper.TeamMapper">
    <update id="quitTeam">
        update user_team
        <set>
            isDelete = 1,
        </set>
        where teamId = #{teamId} and userId = #{userId}
    </update>
</mapper>